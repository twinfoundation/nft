{
	"$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
	"description": "Auto-Generated Release Please configuration for prerelease",
	"pull-request-title-pattern": "chore: prerelease release prepared",
	"pull-request-header": "chore: prerelease release prepared",
	"pull-request-footer": "This PR was generated by the prepare-release GHA",
	"release-type": "node",
	"versioning": "prerelease",
	"prerelease": true,
	"prerelease-type": "next",
	"packages": {
		"packages/nft-models": {
			"package-name": "nft-models",
			"changelog-path": "docs/changelog.md",
			"extra-files": ["src/index.ts"]
		},
		"packages/nft-connector-iota": {
			"package-name": "nft-connector-iota",
			"changelog-path": "docs/changelog.md",
			"extra-files": ["src/index.ts"]
		},
		"packages/nft-connector-iota-stardust": {
			"package-name": "nft-connector-iota-stardust",
			"changelog-path": "docs/changelog.md",
			"extra-files": ["src/index.ts"]
		},
		"packages/nft-connector-entity-storage": {
			"package-name": "nft-connector-entity-storage",
			"changelog-path": "docs/changelog.md",
			"extra-files": ["src/index.ts"]
		},
		"packages/nft-service": {
			"package-name": "nft-service",
			"changelog-path": "docs/changelog.md",
			"extra-files": ["src/index.ts"]
		},
		"packages/nft-rest-client": {
			"package-name": "nft-rest-client",
			"changelog-path": "docs/changelog.md",
			"extra-files": ["src/index.ts"]
		},
		"apps/nft-cli": {
			"package-name": "nft-cli",
			"changelog-path": "docs/changelog.md",
			"extra-files": ["src/cli.ts", "tests/cli.spec.ts", "src/index.ts"]
		}
	},
	"plugins": [
		"node-workspace",
		{
			"type": "linked-versions",
			"groupName": "repo",
			"components": [
				"nft-models",
				"nft-connector-iota",
				"nft-connector-iota-stardust",
				"nft-connector-entity-storage",
				"nft-service",
				"nft-rest-client",
				"nft-cli"
			]
		}
	]
}
